<!DOCTYPE html>
<html>
  <head>
    <title>Reflection</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript" src="//fast.fonts.net/jsapi/b299b159-8854-4490-9537-f0d06d9babb7.js"></script>
  </head>

  <body>
    <nav>
      <% if current_user %>
        <%= link_to "Log Out", logout_path %>
      <% end %>
    </nav>
    <%= yield %>
  </body>
</html>
